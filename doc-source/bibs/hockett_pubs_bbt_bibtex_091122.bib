@article{hockett2017AngleresolvedRABBITTTheory,
  title = {Angle-Resolved {{RABBITT}}: Theory and Numerics},
  author = {Hockett, Paul},
  year = {2017},
  month = aug,
  journal = {Journal of Physics B: Atomic, Molecular and Optical Physics},
  volume = {50},
