@inproceedings{liu2019CombiningStructuralSpectroscopic,
  ids = {CombiningStructuralSpectroscopic},
  title = {Combining {{Structural}} and {{Spectroscopic Probes}} of {{Molecular Dynamics}}},
  booktitle = {{{APS Division}} of {{Atomic}}, {{Molecular}} and {{Optical Physics Meeting Abstracts}}},
  author = {Liu, Yusong and Horton, Spencer and Yang, Jie and Wolf, Thomas and Nunes, Pedro and Forbes, Ruaridh and Makhija, Varun and Hockett, Paul and Lausten, Rune and Stolow, Albert and Marquetand, Philipp and Rozgonyi, Tamas and Wang, Xijie and Weinacht, Thomas},
