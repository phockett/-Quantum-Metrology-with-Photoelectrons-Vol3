  langid = {english},
  keywords = {Publication},
  file = {D\:\\literature\\LiteratureStore\\Optics Letters\\2022\\forbes_et_al_2022_efficient_generation_of_the_7th_harmonic_of_ti.pdf;D\:\\literature\\databases\\PHzot2020_Stimpy\\storage\\YAR3ATEL\\abstract.html}
}

